# professor config file
# format is the same as accepted from Python standard
# library module ConfigParser
[test]
loglevel = debug
logfiles = -

[genpulldata]
number of pull sets = 1
number of test points = -1
use error for comparison = interpolation
loglevel = debug
logfiles = -

[global]
loglevel = info
logfiles = -

[testinter]
add mc runs = 0
mc run step = 2
mc run min = -1
start dim = 1
stop dim = 1
basedir = .
y distribution = twominima
relative error = .05
loglevel = debug
logfiles = -

[interpolation]
method = quadratic
loglevel = warning
logfiles = -

