#! /usr/bin/env bash 
#$ -S /bin/bash
#$ -j oe
#PBS -N ipol-@PROF_JOB

source $HOME/.bashrc

echo "Running Professor interpolation command"

mkdir -p @PROF_OUTDIR/log-interpolate/@PROF_JOB/


python2.5 `which prof-interpolate` \
  --mcdir @PROF_MCDIR \
  --weights @PROF_OBSFILE \
  --runs @PROF_RUNSFILE \
  -o @PROF_IPOLDIR/.. \
  @PROF_ADD_OPTS \
  &> @PROF_OUTDIR/log-interpolate/@PROF_JOB/interpolate.log

exit 0
