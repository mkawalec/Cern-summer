# central professor config file
# format is as accepted from Python standard
# library module ConfigParser
[test]
loglevel = debug
logfiles = -

[genchi2data]
loglevel = debug
number of interpolations = 10
use error for comparison = fake dist
number of test points = 10
logfiles = -

[global]
loglevel = info
logfiles = -

[testinter]
mc run min = 10
mc run step = 2
add mc runs = 0
start dim = 1
stop dim = 2
basedir = .
y distribution = paraboloid
relative error = .05
loglevel = debug
logfiles = -

[interpolation]
loglevel = warning
method = quadratic
logfiles = -

